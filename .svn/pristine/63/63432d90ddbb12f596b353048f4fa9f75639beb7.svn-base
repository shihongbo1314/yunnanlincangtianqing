<script>
import axios from "axios";
const area = [
    "历下区",
    "市中区",
    "天桥区",
    "历城区",
    "槐荫区",
    "长清区",
    "章丘区",
    "济阳区",
    "莱芜区",
    "钢城区",
    "平阴县",
    "商河县",
];
export default {
    data() {
        return {
            shuqian: false,
            xinzen: false,
            table: true,
            checkboxGroup1: [],
            checkboxGroup2: [],
            checkboxGroup3: [],
            checkboxGroup4: [],
            selectData : {
                name : '',
                id : '',
                type : '',
                cteator : '',
            },
            region: "",
            textarea2: "",
            radio: 8,
            radio1: "",
            value: "",
            bookmarkType : [], // 书签类型
            tablePage : { // 表格分页
                allData : [], // 所有数据
                selectData : [], // 查询的数据
                showData : [], // 当前显示的数据
                total : 0, // 总条目数
                pageSize : 15, // 每页显示的数量
                currentPage : 1, // 当前页
            },
            cities: area,
            dialogVisible: false,
            newTemplate: false,
            activeName: "first",
        };
    },
    created() {
        // 初始化界面之前获取数据
        this.initBookMarkType();
    },
    mounted() {
        // 界面显示完成后调用方法
    },
    methods: {
        initBookMarkType(){ // 初始化书签类型
            let url = this.$baseIP + "/sd_product/prod/bookmark/type/list";
            axios.get(url).then((res) => {
                if(res.status == 200 && res.data.state == 200){
                    this.bookmarkType = res.data.records;
                    this.initBookMarkData();
                }
            })      
        },
        getBookMarkType(code){
            for (let i = 0; i < this.bookmarkType.length; i++) {
                if(this.bookmarkType[i].code == code){
                    return this.bookmarkType[i].name;
                }
            }
            return code;
        },
        initBookMarkData(){ // 初始化书签数据
            let url = this.$baseIP + "/sd_product/prod/bookmark/info/list";
            axios.get(url).then((res) => {
                if(res.status == 200 && res.data.state == 200){
                    this.tablePage.allData = res.data.records;
                    this.selectBookMarkData();
                }
            })      
        },
        selectBookMarkData(){ // 按搜索条件查询数据
            this.tablePage.selectData = [];
            let num = 0;
            this.tablePage.allData.forEach(item=>{
                // 判断是否符合查询条件
                if(this.selectData.name != "" && item.name.indexOf(this.selectData.name) == -1){
                    return
                }
                if(this.selectData.id != "" && item.id.indexOf(this.selectData.id) == -1){
                    return
                }
                if(this.selectData.type != "" && item.type.indexOf(this.selectData.type) == -1){
                    return
                }
                if(this.selectData.cteator != "" && item.cteator.indexOf(this.selectData.cteator) == -1){
                    return
                }
                num ++;
                item.num = num;
                item.typeName = this.getBookMarkType(item.type);
                this.tablePage.selectData.push(item);
            })

            this.tablePage.total = this.tablePage.selectData.length;
            this.tablePage.currentPage = 1;
            this.showPage(1);
        },
        showPage(page){ // 表格显示第n页
            let start = (page-1) * this.tablePage.pageSize;
            let end = page * this.tablePage.pageSize;
            this.tablePage.showData = this.tablePage.selectData.slice(start, end);
        },
        handleOpen(key, keyPath) {
            console.log(key, keyPath);
        },
        handleClose(key, keyPath) {
            console.log(key, keyPath);
        },
        deleteRow(index) {
            // 删除书签
            console.log(index)
        },
        handleClick(tab, event) {
            console.log(tab, event);
        },
        NewBookmarks() {
            this.table = false;
            this.xinzen = true;
        },
        returnBokmarks() {
            this.xinzen = false;
            this.table = true; 
        },
    },
};
</script>

